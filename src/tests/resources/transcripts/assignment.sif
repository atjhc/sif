set a to {1, 2, 3}
set a[0] to 10
print a
(-- expect
10 2 3
--)

set a[0...1] to 10, 20
print a
(-- expect
10 20 3
--)

set a to {"one": 1}
set a["one"] to 10
print a["one"]
(-- expect
10
--)

set a to {"one": {"two": 2}}
set a["one"]["two"] to 20
print a["one"]["two"]
(-- expect
20
--)

set a to "one two three"
set a[0] to "1"
print a
(-- expect
1ne two three
--)

set a to "four five six"
set a[5...8] to "5"
print a
(-- expect
four 5 six
--)

set point to {4, 2}
set x, y to point
print x, y
(-- expect
4 2
--)

set a to {"one": 1}
set x, a["one"] to {1, "one"}
print x, a["one"]
(-- expect
1 one
--)

set point to {1, 2}
set point[0], point[1] to {2, 3}
print point
(-- expect
2 3
--)
